<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
	http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/test.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>notuser</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{customerController.goToRegistration()}</from-action>
			<from-outcome>page</from-outcome>
			<to-view-id>/userRegistration.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>petShopHome</from-outcome>
			<to-view-id>/petShopHome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userRegistration.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>petShopHome</from-outcome>
			<to-view-id>/petShopHome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userLoginsRegistration.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>petShopHome</from-outcome>
			<to-view-id>/petShopHome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userHome.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{customerController.goLogout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/shoppingcart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/viewProfile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>history</from-outcome>
			<to-view-id>/viewHistory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/shoppingcart.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/shoppingcart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/viewProfile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>history</from-outcome>
			<to-view-id>/viewHistory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/report.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/shoppingcart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/viewProfile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>history</from-outcome>
			<to-view-id>/viewHistory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/test.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/shoppingcart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/viewProfile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>history</from-outcome>
			<to-view-id>/viewHistory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/history.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/shoppingcart.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/viewProfile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>history</from-outcome>
			<to-view-id>/viewHistory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userHome.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{customerController.goToRegistration()}</from-action>
			<from-outcome>page</from-outcome>
			<to-view-id>userRegistration.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{customerController.goLogout}</from-action>
			<from-outcome>page</from-outcome>
			<to-view-id>adminProduct.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userRegistration.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{registration.customerRegister}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>userLoginsRegistration.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userLoginsRegistration.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{registration.addLogins}</from-action>
			<from-outcome>done</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/userHome.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{customerProduct.goToViewProduct}</from-action>
			<from-outcome>view</from-outcome>
			<to-view-id>viewProduct.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/petShopHome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/userRegistration.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
