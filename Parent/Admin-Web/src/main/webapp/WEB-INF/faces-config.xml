<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
	http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/adminHome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/adminCategory.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>orders</from-outcome>
			<to-view-id>/viewOrders.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>users</from-outcome>
			<to-view-id>/viewUsers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>product</from-outcome>
			<to-view-id>adminProduct.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/adminProduct.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>orders</from-outcome>
			<to-view-id>/viewOrders.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>users</from-outcome>
			<to-view-id>/viewUsers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>category</from-outcome>
			<to-view-id>adminCategory.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/adminHome.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{memberController.goLogout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{memberController.goToCategory}</from-action>
			<from-outcome>page</from-outcome>
			<to-view-id>adminCategory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{memberController.goToProduct}</from-action>
			<from-outcome>page</from-outcome>
			<to-view-id>adminProduct.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>users</from-outcome>
			<to-view-id>viewUsers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>orders</from-outcome>
			<to-view-id>viewOrders.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/viewUsers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>product</from-outcome>
			<to-view-id>/adminProduct.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>category</from-outcome>
			<to-view-id>adminCategory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>adminHome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>orders</from-outcome>
			<to-view-id>viewOrders.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/viewOrders.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>product</from-outcome>
			<to-view-id>/adminProduct.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>category</from-outcome>
			<to-view-id>adminCategory.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>adminHome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>users</from-outcome>
			<to-view-id>viewUsers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
